{
  "name": "standard",
  "license": "ISC",
  "author": " ",
  "scripts": {
    "start2": "koi dev",
    "start": "cross-env SOCKET_SERVER=https://127.0.0.1:8080 koi dev",
    "build": "koi build",
    "postinstall": "koi setup",
    "lint": "koi lint",
    "lint:fix": "koi lint --fix",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "lint:style": "stylelint \"src/**/*.{css,less,scss,sass}\"",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,scss,md,json}'",
    "find-deadcode": "ts-prune -i 'src/.umi' | grep -v 'used in module'",
    "socket-start": "SOCKET_SERVER=https://127.0.0.1:8080 CROSS_ENV koi dev",
    "setup": "koi setup"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": [
      "stylelint --fix --customSyntax postcss-less",
      "git add"
    ],
    "**/*.{scss,sass}": [
      "stylelint --fix --customSyntax postcss-scss",
      "git add"
    ],
    "*.{js,jsx,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "resolutions": {
    "react": "16.14.x",
    "react-dom": "16.14.x",
    "@types/react": "16.x",
    "@types/react-dom": "16.x"
  },
  "dependencies": {
    "ahooks": "^3.9.0",
    "@alife/koi": "^4.4.39",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "plop": "^4.0.1",
    "react": "16.14.x",
    "react-dom": "16.14.x",
    "react-intl": "^5.25.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@alife/magic-lint": "^4.3.1",
    "@types/lodash": "^4.17.16",
    "babel-plugin-import": "^1.13.8",
    "cross-env": "^7.0.3",
    "lint-staged": "^13.2.3",
    "sass": "1.77.6",
    "ts-prune": "^0.10.3",
    "typescript": "^4.9.5",
    "webstorm-disable-index": "^1.2.0",
    "yorkie": "^2.0.0"
  },
  "tnpm": {
    "mode": "yarn",
    "lockfile": "enable"
  },
  "repository": "git@gitlab.alibaba-inc.com:gts-fe/lingting-workbench.git"
}